# Lombok Configuration for Claude Agent SDK

# Add @lombok.Generated annotation to all generated code
# This excludes generated code from code coverage tools
lombok.addLombokGeneratedAnnotation = true

# Add @ConstructorProperties to generated constructors
# This helps with Jackson deserialization
lombok.anyConstructor.addConstructorProperties = true

# Copy Jackson annotations from fields to generated constructor parameters
lombok.copyableAnnotations += com.fasterxml.jackson.annotation.JsonProperty
